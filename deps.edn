{:paths   ["src" "test"]
 :deps    {applied-science/js-interop          {:mvn/version "0.4.2"}
           camel-snake-kebab/camel-snake-kebab {:mvn/version "RELEASE"}
           com.github.pdenno/schema-db         {:mvn/version "RELEASE"}
           cljs-ajax/cljs-ajax                 {:mvn/version "RELEASE"}
           com.cemerick/url                    {:mvn/version "RELEASE"}
           com.taoensso/timbre                 {:mvn/version "RELEASE"}
           com.wsscode/pathom3                 {:mvn/version "2023.01.31-alpha"}
           dk.ative/docjure                    {:mvn/version "RELEASE"}
           datascript/datascript               {:mvn/version "RELEASE"}
           funcool/promesa                     {:mvn/version "10.0.663"} ; My build.
           io.replikativ/datahike              {:mvn/version "0.6.1538"}
           mount/mount                         {:mvn/version "RELEASE"}
           net.clojars.wkok/openai-clojure     {:mvn/version "RELEASE"}
           nrepl/nrepl                         {:mvn/version "RELEASE"}
           org.babashka/sci                    {:mvn/version "RELEASE"}
           org.slf4j/slf4j-nop                 {:mvn/version "RELEASE"} ; Stop spurious logging!
           org.clojure/data.codec              {:mvn/version "RELEASE"}
           org.clojure/data.json               {:mvn/version "RELEASE"}
           org.clojure/data.xml                {:mvn/version "RELEASE"}
           org.clojure/data.zip                {:mvn/version "RELEASE"}}

 ;; See .dir-locals.el for some defaults on CIDER C-c M-j
 :aliases {:dev-hold  {:extra-paths    ["dev" "src/dev" "test"]
                       :jvm-opts       ["-XX:-OmitStackTraceInFastThrow"]
                       :extra-deps     {cider/cider-nrepl           {:mvn/version "RELEASE"}
                                        lilactown/helix             {:mvn/version "RELEASE"}
                                        org.clojure/java.classpath  {:mvn/version "RELEASE"}
                                        org.clojure/tools.namespace {:mvn/version "RELEASE"}}}

           :dev          {:extra-paths    ["dev" "src/dev" "test" "src/rad_mapper/server" "env/dev" "resources"]
                          :jvm-opts       ["-XX:-OmitStackTraceInFastThrow"]
                          :extra-deps     {cider/cider-nrepl               {:mvn/version "RELEASE"}
                                           com.lambdaisland/classpath      {:mvn/version "RELEASE"}
                                           criterium/criterium             {:mvn/version "RELEASE"}
                                           expound/expound                 {:mvn/version "RELEASE"}
                                           integrant/repl                  {:mvn/version "RELEASE"}
                                           pjstadig/humane-test-output     {:mvn/version "RELEASE"}
                                           org.clojure/java.classpath      {:mvn/version "RELEASE"}
                                           org.clojure/tools.namespace     {:mvn/version "RELEASE"}
                                           metosin/muuntaja                {:mvn/version "RELEASE"}
                                           metosin/reitit                  {:mvn/version "RELEASE"}
                                           metosin/ring-http-response      {:mvn/version "RELEASE"}
                                           ring/ring-defaults              {:mvn/version "RELEASE"}
                                           ring/ring-jetty-adapter         {:mvn/version "RELEASE"}
                                           ring/ring-core                  {:mvn/version "RELEASE"}
                                           ring/ring-devel                 {:mvn/version "RELEASE"}
                                           ring/ring-mock                  {:mvn/version "RELEASE"}
                                           ring-cors/ring-cors             {:mvn/version "RELEASE"}
                                           selmer/selmer                   {:mvn/version "1.12.58"}
                                           io.github.kit-clj/kit-core      {:mvn/version "RELEASE"}
                                           io.github.kit-clj/kit-undertow  {:mvn/version "RELEASE"}
                                           io.github.kit-clj/kit-generator {:mvn/version "RELEASE"}
                                           luminus/ring-ttl-session        {:mvn/version "RELEASE"}
                                           luminus-transit/luminus-transit {:mvn/version "RELEASE"
                                                                            :exclusions [com.cognitect/transit-clj]}}}

           :lib-dev   {:extra-paths    ["test" "src/dev"] ; Not needed?
                       :extra-deps     {thheller/shadow-cljs {:mvn/version "RELEASE"}}}

           :test      {:extra-paths    ["test" "src/dev"] ; clojure -X:dev:test
                       :extra-deps
                       {io.github.cognitect-labs/test-runner
                        {:git/tag "v0.5.1" :git/sha "dfb30dd"}}
                       ;; :main-opts ["-m" "cognitect.test-runner"]
                       :exec-fn cognitect.test-runner.api/test
                       :exec-args {:patterns [".*-test.*"]}}

           ;; This for shadow target kaocha-test
           :test-cljs {:extra-paths    ["test" "src/dev"]
                       :extra-deps     {thheller/shadow-cljs      {:mvn/version "RELEASE"}
                                        lambdaisland/kaocha       {:mvn/version "RELEASE"}
                                        lambdaisland/kaocha-cljs2 {:mvn/version "RELEASE"}
                                        com.lambdaisland/glogi    {:mvn/version "RELEASE"}}}

           :deploy    {:replace-deps   {slipset/deps-deploy {:mvn/version "0.2.1"}}
                       :exec-fn        deps-deploy.deps-deploy/deploy
                       :exec-args      {:installer :remote
                                        :sign-releases? false ; signing problem (see mznp deps.edn)
                                        :artifact "RADmapper-1.0.29.jar"}}

           :build    {:deps {io.github.clojure/tools.build {:git/tag "v0.9.4" :git/sha "76b78fe"}}
                      :ns-default build}

           ;; clj -M:morpheus -d graphs -f svg -e 'clojure.core/.*|:clj-kondo/unknown-namespace/.*|:cljs.core/.*' src
           :var-deps-graph {:extra-deps      {thomasa/morpheus {:git/url "https://github.com/benedekfazekas/morpheus.git"
                                                                :git/sha "9d6f2c1ede8be7ab4508065fe6b7e40e0c099ab6"}}
                            :main-opts       ["-m" "thomasa.morpheus.main"]}}}


